# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
alias emacs="emacs -nw"
alias zshconfig="emacs ~/.zshrc"
alias settings="emacs ~/.zshrc"
alias zsh_reload="source ~/.zshrc"
alias reload="source ~/.zshrc"
alias ohmyzsh="emacs .oh-my-zsh"
alias e="emacs"
alias run="./a.out"
alias tty_config="sudo emacs /etc/vsconsole.conf"
alias va="valgrind.sh"
alias open_doc="google-chrome ./documentation/html/index.html"
alias push="./.github/push.sh"
alias plum="~/my_scripts/Plum/plum"
alias c="stty sane && clear"
alias l="eza -la"
alias docker-compose="docker compose"
alias port="sudo netstat -tulnp | grep"
alias discord_update="sudo pacman -Syu ; betterdiscord-installer reinstall"
alias bluetooth="sudo systemctl start bluetooth.service"

# bisous de Dan ;)
alias parrot="curl parrot.live"
alias pornhub="firefox www.pornhub.com"

#xbindkeys
#sudo ~/my_scripts/update_time.sh

# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
